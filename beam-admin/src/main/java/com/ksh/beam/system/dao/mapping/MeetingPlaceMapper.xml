<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ksh.beam.system.dao.MeetingPlaceMapper">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.ksh.beam.system.entity.meeting.Place" id="BaseResultMap">
        <result property="id" column="id"/>
        <result property="typeName" column="type_name"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <!--    分页-->
    <select id="selectPageList" resultType="com.ksh.beam.system.entity.meeting.Place">
        select m.* from meeting_place m order by m.create_time desc
    </select>
</mapper>